{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "autoIncrement": true
      },
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_DEBUG_MODE": "true"
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EXPO_PUBLIC_DEBUG_MODE": "false"
      }
    }
  },
  "submit": {
    "production": {}
  }
}